<UserControl x:Class="Project_for_educational_practice.UserControls.PictureControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Project_for_educational_practice.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="559" d:DesignWidth="800"
             Loaded="PictureLoaded">
    <Grid Background="#f2f2f2">
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Image Grid.Row="0" x:Name="imageMain"/>
    </Grid>
</UserControl>
